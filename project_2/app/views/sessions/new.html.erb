Sign In

<%= form_for @user, url: "/sessions", method: "post" do |f| %>
  <div class="login">
    <%= f.text_field :email, placeholder: "Email", class:'form' %>
     <br />

    <%= f.password_field :password, placeholder: "Password", class:'form' %>
    <br />
    <%= f.submit "Sign In", class:'button' %>
  </div>
  
<% end %> 